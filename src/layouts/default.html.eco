<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <title>
        <%= @document.title %> - wula@php
    </title>
    <link href="/css/style.min.css" rel="stylesheet">
    <link rel="stylesheet" href="//cdn.staticfile.org/github-markdown-css/2.4.1/github-markdown.min.css">
    <link rel="stylesheet" href="//cdn.staticfile.org/highlight.js/9.7.0/styles/github.min.css">
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico">
    <script src="//use.fontawesome.com/c38daec7f7.js"></script>
    <script src="/js/jquery.js"></script>
    <script src="//cdn.staticfile.org/highlight.js/9.7.0/highlight.min.js"></script>
    <script src="/js/script.min.js"></script>
</head>

<body role="flatdoc" class="anima">
    <%- @partial('card') %>

        <div class="header">
            <div class="sidebar-btn">
                <i class="fa fa-align-justify"></i>
            </div>
            <div class="navbar-btn">
                <i class="fa fa-list-ul"></i>
            </div>
            <h1>
                <a href="/" class="big-title">wula@php</a>
                <a class="small-title">
                    <%= @document.doctitle || @document.title %>
                </a>
            </h1>
            <div class="left drop-menu">
                <ul>
                    <li><a href="https://github.com/ninggf/wula">View on GitHub</a></li>
                    <li><a href="https://github.com/ninggf/wula/issues">Issues</a></li>
                    <li><a href="https://github.com/ninggf/wula/issues">有话说</a></li>
                </ul>
            </div>
            <div class="right">
                <iframe src="http://ghbtns.com/github-btn.html?user=ninggf&amp;type=follow&amp;count=true" allowtransparency="true" frameborder="0" scrolling="0" width="170" height="20"></iframe>
                <iframe src="http://ghbtns.com/github-btn.html?user=ninggf&amp;repo=wula&amp;type=watch&amp;count=true" allowtransparency="true" frameborder="0" scrolling="0" width="110" height="20"></iframe>
            </div>

        </div>

        <div class="content-root">
            <div class="menubar menubar-left">
                <div class="section" style="padding:30px 10px">
                    <p style="text-align:center;">wula@php - Just be a happy coder!</p>
                </div>
                <div class="menu section" role="flatdoc-menu">
                    <%- @partial('menu') %>
                </div>
                <div class="bottom section">
                    <p style="text-align:center">
                        <img src="/images/qrcode.png" alt="" width="200" />
                    </p>
                    <p style="text-align:center">
                        <a target="_blank" href="http://shang.qq.com/wpa/qunwpa?idkey=9be37f660c70dd33c22f6055cd113215a52e9cab29d46b5c02fe2f68c67a0f17">扫描加QQ群</a>
                    </p>
                </div>
            </div>

            <div class="menubar menubar-right">
                <div class="section" style="padding:30px 10px">
                    <p style="text-align:center;">
                        <a target="_blank" href="<%= @site.doc_git+@document.relativePath %>">编辑此文档 <i class="fa fa-pencil-square-o" aria-hidden="true"></i></a></p>
                </div>
                <div class="menu section" id="toc" role="flatdoc-menu"></div>
            </div>
            <div class="content markdown-body" id="docbody">
                <%- @content %>
            </div>
            <div class="pn">
                <a id="pre-page" class=""> <i class="fa"></i> </a>
                <a id="nex-page" class=""> <i class="fa"></i> </a>
            </div>
        </div>

</body>

</html>